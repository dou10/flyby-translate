{
    "manifest_version": 2,
    "name": "Fly-By Translate",
    "version": "1.0",
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action":{
        "default_popup": "popup.html",
        "default_title": "FlyBY"
    }
}

